				<style>
					.nav-link {
						transition: background-color 0.3s ease, color 0.3s ease;
						display: flex;
						align-items: center;
						padding: 10px;
						text-decoration: none;
						color: inherit;
					}
					.nav-link.active-yellow {
						background-color: rgba(255, 0, 0, 0.166) !important;
						color: rgb(0, 81, 255) !important;
					}
					.nav-link i, .nav-link-text {
						pointer-events: none;
					}
				</style>
				
				<script>
					function handleNavLinkClick(event) {
						event.preventDefault(); // Prevent immediate navigation
				
						// Remove active-yellow class from all nav links
						document.querySelectorAll('.nav-link').forEach(link => {
							link.classList.remove('active-yellow');
						});
				
						// Add active-yellow class to the clicked link
						const clickedLink = event.currentTarget;
						clickedLink.classList.add('active-yellow');
				
						// Navigate to the link after a short delay to show color change
						setTimeout(() => {
							window.location.href = clickedLink.getAttribute('href');
						}, 200);
					}
				
					// Highlight the currently active page on page load
					document.addEventListener('DOMContentLoaded', () => {
						const currentPath = window.location.pathname;
						const navLinks = document.querySelectorAll('.nav-link');
						
						navLinks.forEach(link => {
							const href = link.getAttribute('href');
							
							// Check if current path matches or is a subset of the link's href
							if (currentPath === href || 
								(href !== '#' && currentPath.includes(href))) {
								// Remove previous active states
								navLinks.forEach(l => l.classList.remove('active-yellow'));
								
								// Add active state to matching link
								link.classList.add('active-yellow');
							}
						});
					});
				</script>